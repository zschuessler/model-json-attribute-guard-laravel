    "orchestra/testbench": "3.5.*|3.6.*|3.7.*|3.8.*",
    "orchestra/database": "3.5.*|3.6.*|3.7.*|3.8.*",
    "phpunit/phpunit": "^6.3|^7.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zschuessler\\ModelJsonAttributeGuard\\ServiceProvider"
      ],
      "aliases": {

      }
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "minimum-stability": "dev"
}
