{
  "name": "zschuessler/model-json-attribute-guard-laravel",
  "description": "An intuitive and fun way to bring consistency to your json columns",
  "type": "library",
  "license": "LGPL-2.1-only",
  "authors": [
    {
      "name": "Zachary Schuessler",
      "email": "zlschuessler@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zschuessler\\ModelJsonAttributeGuard\\": "src/"
    },
    "autoload-dev": {
